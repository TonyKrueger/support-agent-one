# OpenAI
OPENAI_API_KEY=your-openai-api-key-here

# Supabase
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_KEY=your-supabase-anon-key-here
SUPABASE_SERVICE_KEY=your-supabase-service-key-here
SUPABASE_DB_PASSWORD=your-supabase-db-password-here
# Logfire
LOGFIRE_WRITE_TOKEN=your-logfire-write-token-here

# Application
LOG_LEVEL=info  # debug, info, warning, error, critical
ENVIRONMENT=development  # development, staging, production, test

# Test-specific settings (only used when ENVIRONMENT=test)
TEST_MOCK_OPENAI=true  # Set to false to use actual OpenAI API in tests
TEST_MOCK_SUPABASE=true  # Set to false to use actual Supabase in tests 